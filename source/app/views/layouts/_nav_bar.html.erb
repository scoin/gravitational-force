<header>
  <nav>
    <a href=<%= root_path %>><img src="http://imgur.com/IpMWMLg.jpg" id="logo_pic" ></a>

    <ul>
      <% if !logged_in? %>
      <%= link_to "Sign Up", new_user_path, :class => "nav_link" %>
      <%= link_to "Log In", users_path, :class => "nav_link" %>
      <% end %>

      <% if logged_in? %>
      <%= link_to "Sign Out", sign_out_path(current_user), :class => "nav_link"%>
      <% end %>
    </ul>
  </nav>
</header>
